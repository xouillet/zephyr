/dts-v1/;

#include <gigadevice/gd32f403zet6.dtsi>

/ {
	model = "GigaDevice GD32F403Z Evaluation Kit";
	compatible = "gigadevice,gd32f403";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &usart0;
		zephyr,shell-uart = &usart0;
	};
};

&usart0 {
	status = "okay";
};
